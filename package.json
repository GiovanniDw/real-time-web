{
  "name": "vite-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon server/server.js -w server/",
    "start": "NODE_ENV=production node server/server.js",
    "build": "yarn build:server && yarn build:client",
    "preview": "vite preview",
    "build:client": "vite build --outDir docs/client",
    "build:server": "vite build --outDir docs/server --ssr server/server.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vituum/nunjucks": "^0.1.1",
    "express": "^4.18.2",
    "lit-html": "^2.7.2",
    "socket.io-client": "^4.6.1",
    "vite-express": "^0.5.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "cors": "^2.8.5",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "socket.io": "^4.6.1",
    "vite": "^4.3.1"
  }
}