{
  "name": "vite-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/server.js -w server/",
    "start": "NODE_ENV=production node server/server.js",
    "build": "npm run build:server && npm run build:client",
    "preview": "vite preview",
    "build:client": "vite build --outDir docs/client",
    "build:server": "vite build --outDir docs/server --ssr server/server.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vituum/nunjucks": "^0.1.1",
    "express": "^4.18.2",
    "lit-html": "^2.7.2",
    "mongoose": "^7.0.4",
    "socket.io-client": "^4.6.1",
    "vite-express": "^0.5.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "passport-local-mongoose": "^8.0.0",
    "prettier": "^2.8.7",
    "socket.io": "^4.6.1",
    "validator": "^13.9.0",
    "vite": "^4.3.1",
    "vite-plugin-eslint": "^1.8.1"
  }
}
